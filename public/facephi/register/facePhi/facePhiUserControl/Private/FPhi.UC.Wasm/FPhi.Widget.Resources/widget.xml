        font="Raleway-ExtraBold.ttf"
        font_size="14.0"
        align="RIGHT"
        />
        
        <buttonImage id="button_exit" value="cross-black.png"/>
        
        <image id="pagination_on" value="paged-on-black.png"/>
        <image id="pagination_off" value="paged-off-black.png"/>
    </view>
    <view id="StartExtractor">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        mirror_mist_color="#FFFFFF80"
        />
        
        <text id="text"
        color="#939298"
        font="Raleway-Bold.ttf"
        font_size="17.0"
        />
        
        <button id="button_start"
        background="#e9e9e9"
        decorator="#d3d3d3"
        foreground="#757575"
        content="button_start"
        content_type="TEXT_ID"
        font="Raleway-ExtraBold.ttf"
        font_size="14.0"
        align="CENTER"
        />
        
        <buttonImage id="button_info" value="info-black.png"/>
        
        <buttonImage id="button_exit" value="cross-black.png"/>
        
        <image id="logo_medium" value="logo-medium.png"/>
    </view>
    <view id="Extractor">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        />
        
        <video id="extraction_video" value="tipsregistro.mp4"/>
        
        <buttonImage id="button_exit" value="cross-black.png"/>
        
        <image id="logo_medium" value="logo-medium.png"/>
    </view>
