        />
        
        <text id="tip"
        color="#939298"
        font="Raleway-Bold.ttf"
        font_size="17.0"
        />
        
        <text id="text_result"
        font="Raleway-Bold.ttf"
        font_size="30.0"
        />
        
        <text id="text_quality"
        font="Raleway-ExtraBold.ttf"
        font_size="17.0"
        />
        
        <button id="button_repeat"
        background="#f5f5f5"
        decorator="#dcdcdc"
        foreground="#757575"
        content="button_repeat"
        content_type="TEXT_ID"
        font="Raleway-ExtraBold.ttf"
        font_size="14.0"
        align="CENTER"
        />
        
        <button id="button_finish"
