<?xml version="1.0" encoding="UTF-8"?>
<facephi-widget-conf version="1.0"
    color_warning_message="#ff5b55"
    font_warning_message="Raleway-Bold.ttf"
    color_shutter="#00819880"
    color_shutter_text="#FFFFFF"
    font_shutter_text="Raleway-Bold.ttf"
    color_quality_low="#ff5b55"
    color_quality_good="#ffab41"
    color_quality_excellent="#40b305"
    color_progress_bar="#0d9bbb"
    width_progress_bar="6.0">
    
    <view id="RegistrationTips">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        pagination_separator="#d2d2d2"
        mirror_border_color="#d2d2d2"
        mirror_border_width="2.0"
        />
        
        <video id="tip_video" value="tipsluz.mp4"/>
        
        <text id="tip"
        color="#7f7f7f"
        font="Raleway-ExtraBold.ttf"
        font_size="27.0"
        />
        
        <text id="tip_detail"
        color="#7f7f7f"
        font="Raleway-Regular.ttf"
        font_size="17.0"
        />
        
        <button id="button_skip"
        background="#FFFFFF"
        decorator="#FFFFFF"
        foreground="#7f7f7f"
        content="button_skip"
        content_type="TEXT_ID"
        font="Raleway-ExtraBold.ttf"
        font_size="14.0"
        align="LEFT"
        />
        
        <button id="button_next"
        background="#FFFFFF"
        decorator="#FFFFFF"
        foreground="#7f7f7f"
        content="arrow-next-black.png"
        content_type="RESOURCE_ID"
        align="RIGHT"
		width="124px"
        />
        
        <buttonImage id="button_exit" value="cross-black.png"/>
        
        <image id="pagination_on" value="paged-on-black.png"/>
        <image id="pagination_off" value="paged-off-black.png"/>
    </view>
    <view id="FaceMovementTips">
        <background id="background"
        top="#FFFFFF"
